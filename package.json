{
  "name": "junte-ui-app",
  "version": "0.0.1",
  "description": "Junte UI test app",
  "scripts": {
    "sass": "sass",
    "ng": "ng",
    "gulp": "gulp",
    "clean": "rimraf ./dist/junte-ui-app",
    "iconfont": "gulp build --gulpfile ./projects/junte-ui/src/scripts/iconfont/iconfont.ts --cwd ./",
    "serve:lib": "cd ./projects/junte-ui && npm run serve",
    "serve": "wait-on ./dist/junte-ui/bundles/junte-ui.umd.js && ng serve --hmr",
    "start": "rm ./dist/junte-ui/bundles/junte-ui.umd.js && run-p --continue-on-error=false -l -n serve:lib serve",
    "extract-i18n": "ng xi18n --outputPath=locale && xliffmerge --profile xliffmerge.json",
    "build:lib": "cd ./projects/junte-ui && npm run build",
    "build": "npm run clean && npm run build:lib && npm run iconfont && ng build --prod",
    "clean:npm": "npm cache clean --force && rm -rf -- node_modules package-lock.json",
    "refresh": "npm pwdrun clean:package && npm install",
    "processes": "lsof -Pnl +M -i -cmd | grep -E 'LISTEN|TCP|UDP'",
    "e2e": "ng e2e"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "9.0.4",
    "@angular/cdk": "9.1.0",
    "@angular/common": "9.0.4",
    "@angular/compiler": "9.0.4",
    "@angular/core": "9.0.4",
    "@angular/forms": "9.0.4",
    "@angular/localize": "9.0.4",
    "@angular/platform-browser": "9.0.4",
    "@angular/platform-browser-dynamic": "9.0.4",
    "@angular/router": "9.0.4",
    "@ngx-translate/core": "12.1.2",
    "core-js": "3.6.4",
    "date-fns": "2.10.0",
    "ngx-clipboard": "13.0.0",
    "ngx-date-fns": "6.3.0",
    "ngx-highlightjs": "^4.0.2",
    "ngx-logger": "^4.1.8",
    "node-html-parser": "1.2.7",
    "faker": "4.1.0",
    "fs-extra": "8.1.0",
    "js-beautify": "1.10.3",
    "map-stream": "0.0.7",
    "minimist": "1.2.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "6.5.4",
    "tslib": "1.11.1",
    "zone.js": "0.10.2"
  },
  "devDependencies": {
    "@angular/cli": "9.0.4",
    "@angular-devkit/build-angular": "0.900.4",
    "@angular-devkit/build-ng-packagr": "0.900.4",
    "@angular/compiler-cli": "9.0.4",
    "@angular/language-service": "9.0.4",
    "@types/jasmine": "3.5.7",
    "@types/jasminewd2": "2.0.8",
    "@types/node": "13.7.6",
    "change-case": "4.1.1",
    "codelyzer": "5.2.1",
    "del": "5.1.0",
    "fibers": "^4.0.2",
    "gulp": "4.0.0",
    "gulpclass": "0.2.0",
    "gulp-sass": "4.0.2",
    "gulp-clean": "0.4.0",
    "gulp-hash": "4.2.2",
    "gulp-iconfont": "10.0.3",
    "jasmine": "^3.5.0",
    "jasmine-core": "3.5.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "4.4.1",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "2.1.1",
    "karma-jasmine": "3.1.1",
    "karma-jasmine-html-reporter": "1.5.2",
    "ng-packagr": "9.0.2",
    "npm-run-all": "4.1.5",
    "protractor": "5.4.3",
    "sass": "^1.26.2",
    "systemjs": "6.2.5",
    "ts-node": "8.6.2",
    "tslint": "6.0.0",
    "typescript": "3.6.4",
    "wait-on": "4.0.1"
  }
}
