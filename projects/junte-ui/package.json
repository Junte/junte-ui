{
  "name": "@junte/ui",
  "version": "1.0.16",
  "description": "Quality Angular UI components kit",
  "keywords": [
    "angular",
    "components",
    "ui",
    "user interface",
    "ui kit",
    "material"
  ],
  "homepage": "https://junte-ui.com",
  "bugs": "https://gitlab.com/junte/junte-ui/-/issues",
  "license": "MIT",
  "author": {
    "name": "Anton Breslavsky",
    "email": "anton@junte.ru"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/junte/junte-ui.git"
  },
  "scripts": {
    "sass": "sass",
    "ng": "ng",
    "gulp": "gulp",
    "cp": "cp",
    "webpack-bundle-analyzer": "webpack-bundle-analyzer",
    "clean": "rimraf ./dist/junte-ui",
    "styles": "npm run gulp build -- --gulpfile ./src/scripts/styles.ts --cwd ./",
    "iconfont": "npm run gulp build -- --gulpfile ./src/scripts/iconfont/iconfont.ts --cwd ./",
    "encapsulation": "npm run gulp build -- --gulpfile ./node_modules/junte-encapsuler/encapsulation.js --cwd ./",
    "encapsulation:watch": "npm run gulp build -- --gulpfile ./node_modules/junte-encapsuler/encapsulation.js --cwd ./ --watch",
    "assets:create": "rm -rf ./../../dist/junte-ui/lib/assets && mkdir ./../../dist/junte-ui/lib/assets/ && mkdir ./../../dist/junte-ui/lib/assets/icons && mkdir ./../../dist/junte-ui/lib/assets/fonts && mkdir ./../../dist/junte-ui/lib/assets/styles",
    "assets:copy:fonts": "cp -r ./src/lib/assets/fonts/ ./../../dist/junte-ui/lib/assets/fonts/",
    "assets:copy:icons": "cp -r ./src/lib/assets/icons/animated ./../../dist/junte-ui/lib/assets/icons/animated && cp -r ./src/lib/assets/icons/svg ./../../dist/junte-ui/lib/assets/icons/svg",
    "assets:copy:styles": "cp ./src/lib/assets/styles/*.scss ./../../dist/junte-ui/lib/assets/styles",
    "copy:assets": "npm run assets:create && npm run assets:copy:fonts && npm run assets:copy:icons && npm run assets:copy:styles",
    "copy:iconfont": "cd ./src/scripts/iconfont && tsc && cp -r iconfont.js template ../../../../../dist/junte-ui",
    "copy:readme": "cp ./README.md ./../../dist/junte-ui",
    "copy:license": "cp ./../../LICENSE ./../../dist/junte-ui",
    "copy:files": "npm run copy:license && npm run copy:readme && npm run copy:assets && npm run copy:iconfont",
    "build": "npm run clean && npm run encapsulation && npm run ng build junte-ui --prod && npm run iconfont && npm run copy:files && npm run styles",
    "serve": "run-p --continue-on-error=false -l -n *:watch",
    "build:watch": "npm run ng build junte-ui --watch",
    "analize": "npm run clean && npm run encapsulation && npm run ng build junte-ui --prod --stats-json && npm run webpack-bundle-analyzer ../../dist/junte-ui/stats.json",
    "pack": "cd ../../dist/junte-ui && npm pack",
    "publish": "npm run pack && npm publish --tag ${tag:-latest} --access public ./../../dist/junte-ui/junte-ui-$npm_package_version.tgz",
    "publish:unstable": "tag=unstable npm run publish",
    "publish:next": "tag=next npm run publish"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.900.4",
    "@angular-devkit/build-ng-packagr": "0.900.4",
    "@angular/animations": "9.0.4",
    "@angular/cdk": "9.1.0",
    "@angular/cli": "9.0.4",
    "@angular/common": "9.0.4",
    "@angular/compiler": "9.0.4",
    "@angular/compiler-cli": "9.0.4",
    "@angular/core": "9.0.4",
    "@angular/forms": "9.0.4",
    "@angular/language-service": "9.0.4",
    "@angular/localize": "9.0.4",
    "@angular/platform-browser": "9.0.4",
    "@angular/platform-browser-dynamic": "9.0.4",
    "@angular/router": "9.0.4",
    "@ngx-translate/core": "12.1.2",
    "gulp": "4.0.0",
    "gulp-consolidate": "0.2.0",
    "gulp-debug": "4.0.0",
    "gulp-iconfont": "10.0.1",
    "gulp-rename": "1.4.0",
    "gulpclass": "0.2.0",
    "junte-encapsuler": "0.0.40",
    "map-stream": "0.0.7",
    "minimist": "1.2.5",
    "ng-packagr": "9.0.2",
    "npm-run-all": "4.1.5",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.3",
    "typescript": "3.6.4",
    "ts-node": "8.6.2",
    "query-ast": "1.0.3",
    "scss-parser": "1.0.4",
    "webpack-bundle-analyzer": "^3.8.0",
    "zone.js": "0.10.2"
  },
  "dependencies": {
    "date-fns": "2.10.0",
    "ngx-date-fns": "6.3.0",
    "ngx-logger": "4.1.8"
  },
  "peerDependencies": {
    "@angular/common": "^9.0.0",
    "@angular/core": "^9.0.0",
    "@angular/animations": "^9.0.0",
    "@angular/forms": "^9.0.0",
    "@angular/router": "^9.0.0",
    "@ngx-translate/core": "12.1.2",
    "rxjs": "^6.5.3"
  }
}
