{
  "composition": {
    "section": "forms",
    "pathFrom": "../../assets/styles",
    "pathTo": "../../../../../../dist/junte-ui/lib/assets/styles",
    "from": [
      "filter/_filter.scss"
    ],
    "to": "_filter.scss"
  },
  "encapsuler": [
    {
      "host": "jnt-filter-host",
      "html": {
        "from": "filter.component.html",
        "to": "filter.encapsulated.html"
      },
      "scss": {
        "from": "filter.component.scss",
        "to": "../../assets/styles/forms/filter/_filter.scss"
      }
    }
  ]
}
