{
  "composition": {
    "section": "forms",
    "pathFrom": "../../assets/styles/components",
    "pathTo": "../../../../../../../dist/junte-ui/lib/assets/styles/components",
    "from": [
      "checkbox/checkbox.scss",
      "checkbox/checkbox-group/checkbox-group.scss"
    ],
    "to": "checkbox.scss"
  },
  "encapsuler": [
    {
      "host": "jnt-checkbox-host",
      "html": {
        "from": "checkbox.component.html",
        "to": "checkbox.encapsulated.html"
      },
      "scss": {
        "from": "checkbox.component.scss",
        "to": "../../assets/styles/components/forms/checkbox/checkbox.scss"
      }
    },
    {
      "host": "jnt-checkbox-group-host",
      "html": {
        "from": "checkbox-group/checkbox-group.component.html",
        "to": "checkbox-group/checkbox-group.encapsulated.html"
      },
      "scss": {
        "from": "checkbox-group/checkbox-group.component.scss",
        "to": "../../assets/styles/components/forms/checkbox/checkbox-group/checkbox-group.scss"
      }
    }
  ]
}
