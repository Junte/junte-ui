{
  "composition": {
    "section": "forms",
    "pathFrom": "../../assets/styles",
    "pathTo": "../../../../../../dist/junte-ui/lib/assets/styles",
    "from": [
      "button/button.scss",
      "button/group/button-group.scss"
    ],
    "to": "button.scss"
  },
  "encapsuler": [
    {
      "host": "jnt-button-host",
      "html": {
        "from": "button.component.html",
        "to": "button.encapsulated.html"
      },
      "scss": {
        "from": "button.component.scss",
        "to": "../../assets/styles/forms/button/button.scss"
      }
    },
    {
      "host": "jnt-button-group-host",
      "html": {
        "from": "group/button-group.component.html",
        "to": "group/button-group.encapsulated.html"
      },
      "scss": {
        "from": "group/button-group.component.scss",
        "to": "../../assets/styles/forms/button/group/button-group.scss"
      }
    }
  ]
}
