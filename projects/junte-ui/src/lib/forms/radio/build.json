    "section": "forms",
    "pathFrom": "../../assets/styles",
    "pathTo": "../../../../../../dist/junte-ui/lib/assets/styles",
    "from": [
      "radio/radio.scss",
      "radio/radio-group/radio-group.scss"
    ],
    "to": "radio.scss"
  },
  "encapsuler": [
    {
      "host": "jnt-radio-host",
      "html": {
        "from": "radio.component.html",
        "to": "radio.encapsulated.html"
      },
      "scss": {
        "from": "radio.component.scss",
        "to": "../../assets/styles/forms/radio/radio.scss"
      }
    },
    {
      "host": "jnt-radio-group-host",
      "html": {
        "from": "radio-group/radio-group.component.html",
        "to": "radio-group/radio-group.encapsulated.html"
      },
      "scss": {
        "from": "radio-group/radio-group.component.scss",
        "to": "../../assets/styles/forms/radio/radio-group/radio-group.scss"
      }
    }
  ]
}
