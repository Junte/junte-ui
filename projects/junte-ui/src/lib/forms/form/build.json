{
  "composition": {
    "section": "forms",
    "pathFrom": "../../assets/styles",
    "pathTo": "../../../../../../dist/junte-ui/lib/assets/styles",
    "from": [
      "form/_form.scss",
      "form/item/_form-item.scss",
      "form/control/_form-control.scss",
      "form/label/_form-label.scss",
      "form/message/_form-message.scss"
    ],
    "to": "_form.scss"
  },
  "encapsuler": [
    {
      "host": "jnt-form-host",
      "html": {
        "from": "form.component.html",
        "to": "form.encapsulated.html"
      },
      "scss": {
        "from": "form.component.scss",
        "to": "../../assets/styles/forms/form/_form.scss"
      }
    },
    {
      "host": "jnt-form-item-host",
      "html": {
        "from": "item/form-item.component.html",
        "to": "item/form-item.encapsulated.html"
      },
      "scss": {
        "from": "item/form-item.component.scss",
        "to": "../../assets/styles/forms/form/item/_form-item.scss"
      }
    },
    {
      "host": "jnt-form-control-host",
      "html": {
        "from": "control/form-control.component.html",
        "to": "control/form-control.encapsulated.html"
      },
      "scss": {
        "from": "control/form-control.component.scss",
        "to": "../../assets/styles/forms/form/control/_form-control.scss"
      }
    },
    {
      "host": "jnt-form-label-host",
      "html": {
        "from": "label/form-label.component.html",
        "to": "label/form-label.encapsulated.html"
      },
      "scss": {
        "from": "label/form-label.component.scss",
        "to": "../../assets/styles/forms/form/label/_form-label.scss"
      }
    },
    {
      "host": "jnt-form-message-host",
      "html": {
        "from": "message/form-message.component.html",
        "to": "message/form-message.encapsulated.html"
      },
      "scss": {
        "from": "message/form-message.component.scss",
        "to": "../../assets/styles/forms/form/message/_form-message.scss"
      }
    }
  ]
}
