{
  "composition": {
    "section": "overlays",
    "pathFrom": "../../assets/styles",
    "pathTo": "../../../../../../dist/junte-ui/lib/assets/styles",
    "from": [
      "modal/_modal.scss"
    ],
    "to": "_modal.scss"
  },
  "encapsuler": [
    {
      "host": "jnt-modal-host",
      "html": {
        "from": "modal.component.html",
        "to": "modal.encapsulated.html"
      },
      "scss": {
        "from": "modal.component.scss",
        "to": "../../assets/styles/overlays/modal/_modal.scss"
      }
    }
  ]
}
