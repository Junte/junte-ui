{
  "composition": {
    "section": "layout",
    "pathFrom": "../../assets/styles",
    "pathTo": "../../../../../../dist/junte-ui/lib/assets/styles",
    "from": [
      "lp/slide/_lp-slide.scss",
      "lp/layout/_lp-layout.scss",
      "lp/rewind/_lp-rewind.scss",
      "lp/header/_lp-header.scss",
      "lp/footer/_lp-footer.scss"
    ],
    "to": "_lp.scss"
  },
  "encapsuler": [
    {
      "host": "jnt-lp-layout-host",
      "html": {
        "from": "layout/lp-layout.component.html",
        "to": "layout/lp-layout.encapsulated.html"
      },
      "scss": {
        "from": "layout/lp-layout.component.scss",
        "to": "../../assets/styles/layout/lp/layout/_lp-layout.scss"
      }
    },
    {
      "host": "jnt-lp-rewind-host",
      "html": {
        "from": "rewind/lp-rewind.component.html",
        "to": "rewind/lp-rewind.encapsulated.html"
      },
      "scss": {
        "from": "rewind/lp-rewind.component.scss",
        "to": "../../assets/styles/layout/lp/rewind/_lp-rewind.scss"
      }
    },
    {
      "host": "jnt-lp-slide-host",
      "html": {
        "from": "slide/lp-slide.component.html",
        "to": "slide/lp-slide.encapsulated.html"
      },
      "scss": {
        "from": "slide/lp-slide.component.scss",
        "to": "../../assets/styles/layout/lp/slide/_lp-slide.scss"
      }
    },
    {
      "host": "jnt-lp-header-host",
      "html": {
        "from": "header/lp-header.component.html",
        "to": "header/lp-header.encapsulated.html"
      },
      "scss": {
        "from": "header/lp-header.component.scss",
        "to": "../../assets/styles/layout/lp/header/_lp-header.scss"
      }
    },
    {
      "host": "jnt-lp-footer-host",
      "html": {
        "from": "footer/lp-footer.component.html",
        "to": "footer/lp-footer.encapsulated.html"
      },
      "scss": {
        "from": "footer/lp-footer.component.scss",
        "to": "../../assets/styles/layout/lp/footer/_lp-footer.scss"
      }
    }
  ]
}
