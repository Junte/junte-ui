      }
    },
    {
      "host": "jnt-lp-slide-host",
      "html": {
        "from": "slide/lp-slide.component.html",
        "to": "slide/lp-slide.encapsulated.html"
      },
      "scss": {
        "from": "slide/lp-slide.component.scss",
        "to": "../../assets/styles/layout/lp/slide/lp-slide.scss"
      }
    },
    {
      "host": "jnt-lp-header-host",
      "html": {
        "from": "header/lp-header.component.html",
        "to": "header/lp-header.encapsulated.html"
      },
      "scss": {
        "from": "header/lp-header.component.scss",
        "to": "../../assets/styles/layout/lp/header/lp-header.scss"
      }
    }
  ]
