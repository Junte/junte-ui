{
  "composition": {
    "section": "layout",
    "pathFrom": "../../assets/styles",
    "pathTo": "../../../../../../dist/junte-ui/lib/assets/styles",
    "from": [
      "lp/slide/lp-slide.scss",
      "lp/layout/lp-layout.scss",
      "lp/rewind/lp-rewind.scss",
      "lp/header/lp-header.scss"
    ],
    "to": "lp.scss"
  },
  "encapsuler": [
    {
      "host": "jnt-lp-layout-host",
      "html": {
