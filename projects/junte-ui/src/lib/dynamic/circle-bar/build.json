{
  "composition": {
    "section": "dynamic",
    "pathFrom": "../../assets/styles",
    "pathTo": "../../../../../../dist/junte-ui/lib/assets/styles",
    "from": [
      "circle-bar/_circle-bar.scss",
      "circle-bar/indicator-group/_indicator-group.scss",
      "circle-bar/indicator/_indicator.scss"
    ],
    "to": "_circle-bar.scss"
  },
  "encapsuler": [
    {
      "host": "jnt-circle-bar-host",
      "html": {
        "from": "circle-bar.component.html",
        "to": "circle-bar.encapsulated.html"
      },
      "scss": {
        "from": "circle-bar.component.scss",
        "to": "../../assets/styles/dynamic/circle-bar/_circle-bar.scss"
      }
    },
    {
      "host": "jnt-bar-indicator-group-host",
      "html": {
        "from": "indicator-group/indicator-group.component.html",
        "to": "indicator-group/indicator-group.encapsulated.html"
      },
      "scss": {
        "from": "indicator-group/indicator-group.component.scss",
        "to": "../../assets/styles/dynamic/circle-bar/indicator-group/_indicator-group.scss"
      }
    },
    {
      "host": "jnt-bar-indicator-host",
      "html": {
        "from": "indicator/indicator.component.html",
        "to": "indicator/indicator.encapsulated.html"
      },
      "scss": {
        "from": "indicator/indicator.component.scss",
        "to": "../../assets/styles/dynamic/circle-bar/indicator/_indicator.scss"
      }
    }
  ]
}
